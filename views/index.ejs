<div class="trees"></div>
  <div class="container">
    <h1 class="text-center">The Original 151 Pokemon</h1>


    <div class="section">
      <% pokemon.forEach(function(pokemon) { %>
        <div class="well">
          <h2 id='pokemonNames'><%= pokemon.name %></h2>
          <form method="POST" action="/pokemon">
            <input id='pokeNames' hidden type="text" name="name" value="<%= pokemon.name %>">
            <button class="btn btn-primary" type="submit">Add to Favorites</button>
          </form>
        </div>
      <% }); %>
    </div>
  </div>
